{% block head_brand %}
	<meta http-equiv="content-type" content="text/html; charset=utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<title>{{ title }}</title>
	<meta name="description" content="{{ description }}" />

	<link rel="apple-touch-icon" sizes="180x180" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/favicon-16x16.png">
    <link rel="manifest" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/site.webmanifest">
    <link rel="mask-icon" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/safari-pinned-tab.svg" color="#e3012b">
    <link rel="shortcut icon" href="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/favicon.ico">
    <meta name="msapplication-TileColor" content="#e3012b">
    <meta name="msapplication-config" content="https://www.illustre.ch/themes/custom/illustre/assets/img/favicons/browserconfig.xml">
    <meta name="theme-color" content="#e3012b">

	<!-- CHARTBEAT -->
	<script type='text/javascript'>var _sf_startpt=(new Date()).getTime()</script>

	<!-- FACEBOOK -->
	<link rel="image_src" href="{{ app.url ~ imgSocial }}" />
	<meta property="og:title" content="{{ title }}" />
	<meta property="og:site_name" content="illustre.ch" />
	<meta property="og:image" content="{{ app.url ~ imgSocial }}" />
	<meta property="og:type" content="article" />
	<meta property="og:url" content="{{ app.url }}" />
	<meta property="og:description" content="{{ description }}" />

	<!-- TWITTER -->
	<meta name="twitter:card" content="summary_large_image" />
	<meta name="twitter:site" content="@lillustre" />
	<meta name="twitter:description" content="{{ description }}" />
	<meta name="twitter:image" content="{{ app.url ~ imgSocial }}" />
	<meta name="twitter:url" content="{{ app.url }}" />

	<!-- CANONICAL URL -->
	<link rel="canonical" href="{{ app.url }}"/>

	<!-- CSS -->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt" crossorigin="anonymous">


	<!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">-->
	<style>
		/* RESET BROWSER HTML5 */
		html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent;margin:0;padding:0}body{line-height:1}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}nav ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:none}a{font-size:100%;vertical-align:baseline;background:transparent;margin:0;padding:0}ins{background-color:#ff9;color:#000;text-decoration:none}mark{background-color:#ff9;color:#000;font-style:italic;font-weight:700}del{text-decoration:line-through}abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help}table{border-collapse:collapse;border-spacing:0}hr{display:block;height:1px;border:0;border-top:1px solid #ccc;margin:1em 0;padding:0}input,select{vertical-align:middle}ul{list-style-type:none;}
		
		/* BAR */
		div.brand-nav { width: 100%; background: rgba(0, 0, 0, 0.15); position: fixed; height: 57px; z-index: 1000; top:0;}
		/* div.brand-nav { width: 100%; background: linear-gradient(to bottom, rgba(0,0,0,.7) 0, rgba(0,0,0,.5) 50%, rgba(0,0,0,.0) 200%); position: fixed; height: 57px; z-index: 1000;} */
			div.brand-nav.normal { width: 100%; background: white; box-shadow:0 1px 7px 0 rgba(0,0,0,0.1); }

		/* NAVBAR HIDE EFFECT CLASSES (HEADROOM.JS) */
		div.headroom { transition: transform 200ms linear; }
		div.headroom--pinned { transform: translateY(0%); -webkit-transition: all 0.2s ease-in; -moz-transition: all 0.2s ease-in; transition: all 0.2s ease-in; opacity: 1;}
		div.headroom--unpinned { transform: translateY(-100%); -webkit-transition: all 0.2s ease-in; -moz-transition: all 0.2s ease-in; transition: all 0.2s ease-in; opacity: 0;}

		/* IF NAVBAR HAS "LOCKED" CLASS, IT DOESNT HIDE ANYMORE (set in variable.json) */
		div.brand-nav.headroom.is-locked { transform: translateY(0%) !important; opacity: 1 !important; }
		div.brand-nav.headroom.is-locked div.brand-logo-wrapper { transform: translateX(0%) !important; }
		div.brand-nav.headroom.is-locked div.brand-social-wrapper { transform: translateX(0%) !important; }

		/* BRAND LOGO WRAPPER + SLIDE LEFT EFFECT CLASSES */
		div.brand-nav div.brand-logo-wrapper { height: 100%; float: left; }
		div.brand-nav div.brand-logo-wrapper.is-slided { transform: translateX(-20%); -webkit-transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); -moz-transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); }
		div.brand-nav div.brand-logo-wrapper.isnot-slided { transform: translateX(0%); -webkit-transition: all 0.25s ease-in; -moz-transition: all 0.25s ease-in; transition: all 0.25s ease-in; }
		div.brand-nav div.brand-logo-wrapper a { height: 100%; float: left; display: block; margin-left: 0px; text-decoration: none; border: none;}
		div.brand-nav div.brand-logo-wrapper svg#brand-logo { height: 100%; height: auto; }
			div.brand-nav.normal div.brand-logo-wrapper svg#brand-logo path { fill: #930025; }
		div.brand-nav div.brand-logo-wrapper .partner-brand-logo { width: auto; height: 35px; margin-top: 12px; }

		/* SOCIAL WRAPPER + SLIDE RIGHT EFFECT CLASSES */
		div.brand-nav div.brand-social-wrapper { float: right; height: 100%; }
		div.brand-nav div.brand-social-wrapper.is-slided { transform: translateX(20%); -webkit-transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); -moz-transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); transition: all 0.2s cubic-bezier(.6,-1.09,.36,1.72); }
		div.brand-nav div.brand-social-wrapper.isnot-slided { transform: translateX(0%); -webkit-transition: all 0.25s ease-in; -moz-transition: all 0.25s ease-in; transition: all 0.25s ease-in; }
		
		/* SOCIAL WRAPPER > BUTTONS */
		div.brand-nav div.brand-social-wrapper span.brand-button { width: 60px; height: 100%; /*background: red;*/ float: right; color: white; text-align: center; cursor: pointer; -webkit-transition: all 0.1s ease-in; -moz-transition: all 0.1s ease-in; transition: all 0.1s ease-in; }
		div.brand-nav div.brand-social-wrapper a { float: right; text-decoration: none; border: none; height: 100%; }
		div.brand-nav div.brand-social-wrapper a:nth-child(1) { margin-right: 30px; }
		div.brand-nav div.brand-social-wrapper span.brand-button:hover { background: rgba(255, 255, 255, .3); -webkit-transition: all 0.1s ease-in; -moz-transition: all 0.1s ease-in; transition: all 0.1s ease-in;}
		
		/* SOCIAL WRAPPER > BUTTONS > ICONS */
		div.brand-nav div.brand-social-wrapper span.brand-button i { font-size: 20px; margin-top: 18px; -webkit-transition: all 0.1s ease-in; -moz-transition: all 0.1s ease-in; transition: all 0.1s ease-in; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button i.fa-facebook-f { color: #a2a2a2; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button:hover i.fa-facebook-f { color: #4366b2; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button i.fa-twitter { color: #a2a2a2; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button:hover i.fa-twitter { color: #00aced; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button i.fa-envelope { color: #a2a2a2; }
			div.brand-nav.normal div.brand-social-wrapper span.brand-button:hover i.fa-envelope { color: #e3002c; }
		div.brand-nav div.brand-social-wrapper span.brand-button:hover i { font-size: 30px; margin-top: 13px; -webkit-transition: all 0.1s ease-in; -moz-transition: all 0.1s ease-in; transition: all 0.1s ease-in;}
		/*div.brand-nav div.brand-social-wrapper span.brand-button i.fa-share-alt { clear: both !important; }*/

		/* MEDIAQUERIES */
		@media screen and (max-width: 580px) {
			div.brand-nav { height: 40px; }
		  	div.brand-nav div.brand-social-wrapper a:nth-child(1) { margin-right: 0; }
		  	div.brand-nav div.brand-logo-wrapper a { margin-left: 0; }
		  	div.brand-nav div.brand-logo-wrapper svg#brand-logo { height: 100% !important; height: auto; }
		  	div.brand-nav div.brand-logo-wrapper .partner-brand-logo { height: 24px; margin-top: 10px;}
		  	div.brand-nav div.brand-social-wrapper span.brand-button { width: 40px; }
		  	div.brand-nav div.brand-social-wrapper span.brand-button i { font-size: 15px; margin-top: 13px; }
		  	div.brand-nav div.brand-social-wrapper span.brand-button:hover i { font-size: 22px; margin-top: 10px; }
		}
	</style>

